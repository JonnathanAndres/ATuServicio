<% @sel_providers.each do |provider| %>
<tr id="<%= provider.id %>">
  <td>
    <h4><%= provider.nombre_completo %></h4>
    <p>
      <a href="http://<%= provider.web %>" title="Sitio web" target="_blank">
        <%= provider.web %>
      </a>
    </p>
    <p>
      <% if @selected_state %>
        <p>Estructura:</p>
        <span>
          Primaria: <%= provider.coverage_by_state(@selected_state, 'Sede Central') %>
        </span>
        <span>
          Secundaria: <%= provider.coverage_by_state(@selected_state, 'Sede Secundaria') %>
        </span>
        <span>
          Ambulatorio: <%= provider.coverage_by_state(@selected_state, 'Ambulatorio') %>
        </span>
        <span>
          Urgencia: <%= provider.coverage_by_state(@selected_state, 'Urgencia') %>
        </span>
      <% else %>
        <p class="structure_icons">
          <% structure = provider_structure(provider) %>
          <span class="estructura">Estructura:</span>
          <% if structure[:primaria] > 0 %>
            <span class="round-btn">1</span>
          <% end %>
          <% if structure[:secundaria] > 0 %>
            <span class="round-btn">2</span>
            <% if structure[:secundaria] > 0 %>
              <span class="float_left">X <%= structure[:secundaria] %></span>
            <% end %>
          <% end %>
        <% if structure[:ambulatorio] > 0 %>
          <span class="round-btn">P</span>
          <% if (structure[:ambulatorio] > 1) %>
            <span class="float_left">X <%= structure[:ambulatorio] %></span>
          <% end %>
        <% end %>
        <% if structure[:urgencia] %>
          <span class="round-btn magenta">U</span>
        <% end %>
        <p class="sedes">Sedes en: <%= provider.states.join(", ") %></p>
      </p>
  <% end %>
  <%= link_to 'Comparar', compare_path(selected_providers: "#{provider.id}") %>
</td>
<td>
  <%= show_if_valid(provider, :tiempo_espera_medicina_general) %><br/>
  <%= show_if_valid(provider, :tiempo_espera_pediatria) %><br/>
  <%= show_if_valid(provider, :tiempo_espera_cirugia_general) %><br/>
  <%= show_if_valid(provider, :tiempo_espera_ginecotocologia) %><br/>
  <%= show_if_valid(provider, :tiempo_espera_medico_referencia) %><br/>
  <strong><%= provider.sums_provider %> / <%= Provider.maximums %></strong>
</td>
<td>
  <%= number_with_precision(provider.satisfaccion_primer_nivel_atencion_2014, precision: 2) %><br/>
  <%= number_with_precision(provider.conformidad_disponibilidad_agenda_2014, precision: 2) %><br/>
  <%= number_with_precision(provider.facilidad_para_realizar_tramites_gestiones_2014, precision: 2) %><br/>
  <%= number_with_precision(provider.disponibilidad_medicamentos_farmacia_2014, precision: 2) %><br/>
  <%= number_with_precision(provider.queja_sugerencia_sabe_donde_dirigirse_2014, precision: 2) %><br/>
</td>
<td>
  <%= number_with_precision(provider.average(:medicamentos), precision: 2) %><br/>
  <%= provider.average(:tickets) %><br/>
  <%= provider.average(:tickets_urgentes) %><br/>
  <%= provider.average(:estudios) %><br/>
</td>
<td>
  <%= raw progress_bar(provider.meta_medico_referencia, 'celeste') %>
  <%= raw progress_bar(provider.meta_ninos_controlados, 'marino') %>
  <%= raw progress_bar(provider.meta_embarazadas, 'azul') %>
</td>
<td><%=provider.afiliados%></td>
</tr>
<% end %>
